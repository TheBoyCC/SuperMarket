                                <span id="notBadge" class="bs-badge badge-absolute float-right bg-red" visible="false" runat="server"></span>
                                <i class="glyph-icon icon-bell-o"></i>
                            </a>
                            <div class="dropdown-menu box-md float-right">
                                <div id="divNotID" class="popover-title display-block clearfix pad10A" runat="server">
                                    No New Notifications                                                  
                                </div>
                                <div id="divNotification" visible="false" class="scrollable-content scrollable-slim-box" runat="server">
